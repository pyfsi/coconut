{
  "settings": {
    "timestep_start": 0,
    "number_of_timesteps": 100,
    "delta_t": 0.01,
    "save_restart": 0
  },
  "coupled_solver": {
    "type": "coupled_solvers.iqni",
    "settings": {
      "omega": 0.05,
      "write_results": 100,
      "model": {
        "type": "coupled_solvers.models.ls",
        "settings": {
          "min_significant": 1e-12,
          "q": 10
        }
      }
    },
    "predictor": {
      "type": "predictors.linear"
    },
    "convergence_criterion": {
      "type": "convergence_criteria.or",
      "settings": {
        "criteria_list": [
          {
            "type": "convergence_criteria.iteration_limit",
            "settings": {
              "maximum": 15
            }
          },
          {
            "type": "convergence_criteria.relative_norm",
            "settings": {
              "tolerance": 1e-6,
              "order": 2
            }
          }
        ]
      }
    },
    "solver_wrappers": [
      {
        "type": "solver_wrappers.python.tube_flow_solver",
        "settings": {
          "working_directory": "CFD",
          "input_file": "solver_parameters_velocity.json",
          "interface_input": [
            {
              "model_part": "wall",
              "variables": [
                "displacement"
              ]
            }
          ],
          "interface_output": [
            {
              "model_part": "wall",
              "variables": [
                "pressure",
                "traction"
              ]
            }
          ]
        }
      },
      {
        "type": "solver_wrappers.python.tube_ringmodel_solver",
        "settings": {
          "working_directory": "CSM",
          "input_file": "solver_parameters.json",
          "interface_input": [
            {
              "model_part": "wall",
              "variables": [
                "pressure",
                "traction"
              ]
            }
          ],
          "interface_output": [
            {
              "model_part": "wall",
              "variables": [
                "displacement"
              ]
            }
          ]
        }
      }
    ]
  }
}
