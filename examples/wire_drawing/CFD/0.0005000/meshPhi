FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceScalarField;
    location    "0.0005000";
    object      meshPhi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    dieWall
    {
        type            calculated;
        value           uniform 0;
    }
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
    Wire
    {
        type            calculated;
        value           uniform 0;
    }
    inlet
    {
        type            calculated;
        value           uniform 0;
    }
    back
    {
        type            wedge;
        value           uniform 0;
    }
    front
    {
        type            wedge;
        value           uniform 0;
    }
}


