{
  "settings": {
    "number_of_timesteps": 1
  },
  "coupled_solver": {
    "type": "coupled_solvers.iqni",
    "settings": {
      "delta_t": 1.0,
      "timestep_start": 0,
      "save_results": true,
      "omega": 0.05,
      "model": {
        "type": "coupled_solvers.models.ls",
        "settings": {
          "min_significant": 0,
          "q": 0
        }
      }
    },
    "predictor": {
      "type": "predictors.linear"
    },
    "convergence_criterion": {
      "type": "convergence_criteria.or",
      "settings": {
        "criteria_list": [
          {
            "type": "convergence_criteria.iteration_limit",
            "settings": {
              "maximum": 20
            }
          },
          {
            "type": "convergence_criteria.absolute_norm",
            "settings": {
              "tolerance": 1e-9,
              "order": 2
            }
          }
        ]
      }
    },
    "solver_wrappers": [
      {
        "type": "solver_wrappers.fluent.v2019R1",
        "settings": {
          "working_directory": "CFD",
          "case_file": "case_tube2d.cas",
          "dimensions": 2,
          "unsteady": false,
          "thread_names": [
            "beamoutside"
          ],
          "interface_input": {
            "beamoutside_nodes": "DISPLACEMENT"
          },
          "interface_output": {
            "beamoutside_faces": [
              "PRESSURE",
              "TRACTION"
            ]
          },
          "cores": 4,
          "fluent_gui": false,
          "max_nodes_per_face": 2,
          "hybrid_initialization": false,
          "flow_iterations": 200,
          "save_iterations": 1
        }
      },
      {
        "type": "solver_wrappers.mapped",
        "settings": {
          "solver_wrapper": {
            "type": "solver_wrappers.abaqus.614",
            "settings": {
              "working_directory": "CSM",
              "cores": 1,
              "dimensions": 2,
              "arraysize": 1000000.0,
              "surfaces": 1,
              "ramp": true,
              "subcycling": true,
              "minInc": 0.0,
              "initialInc": 1.0,
              "maxNumInc": 500,
              "maxInc": 1.0,
              "surfaceIDs": [
                "BEAMINSIDEMOVING"
              ],
              "interface_input": {
                "BEAMINSIDEMOVING_load_points": [
                  "PRESSURE",
                  "TRACTION"
                ]
              },
              "interface_output": {
                "BEAMINSIDEMOVING_nodes": "DISPLACEMENT"
              },
              "mp_mode": "THREADS",
              "input_file": "Base.inp",
              "save_iterations": 1
            }
          },
          "mapper_interface_input": {
            "type": "mappers.interface",
            "settings": {
              "type": "mappers.combined",
              "settings": {
                "mappers": [
                  {
                    "type": "mappers.permutation",
                    "settings": {
                      "permutation": [
                        1,
                        0,
                        2
                      ]
                    }
                  },
                  {
                    "type": "mappers.radial_basis",
                    "settings": {
                      "balanced_tree": false,
                      "directions": [
                        "X",
                        "Y"
                      ],
                      "parallel": true
                    }
                  }
                ]
              }
            }
          },
          "mapper_interface_output": {
            "type": "mappers.interface",
            "settings": {
              "type": "mappers.combined",
              "settings": {
                "mappers": [
                  {
                    "type": "mappers.radial_basis",
                    "settings": {
                      "balanced_tree": false,
                      "directions": [
                        "X",
                        "Y"
                      ],
                      "parallel": true
                    }
                  },
                  {
                    "type": "mappers.permutation",
                    "settings": {
                      "permutation": [
                        1,
                        0,
                        2
                      ]
                    }
                  }
                ]
              }
            }
          }
        }
      }
    ]
  }
}