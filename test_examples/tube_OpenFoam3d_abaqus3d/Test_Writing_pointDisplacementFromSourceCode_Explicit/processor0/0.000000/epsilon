FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.000000";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 14.855;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 14.855;
    }
    outlet
    {
        type            zeroGradient;
    }
    mantle
    {
        type            epsilonWallFunction;
        value           uniform 14.855;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 14.855;
    }
}


