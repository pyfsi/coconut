{
  "solver_wrappers": [
    {
      "type": "solver_wrappers.python.tube_structure_solver",
      "settings": {
        "working_directory": "solver_wrappers/python/tube/test_tube_structure/CSM",
        "input_file": "solver_parameters.json",
        "delta_t": 0.0001,
        "interface_input": [
          {
            "model_part": "wall",
            "variables": [
              "pressure",
              "traction"
            ]
          }
        ],
        "interface_output": [
          {
            "model_part": "wall",
            "variables": [
              "displacement"
            ]
          }
        ]
      }
    }
  ]
}
