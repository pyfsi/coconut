FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
       type            uniformTotalPressure;
        p0               table
    (
    	(0 1.3332)  
	    (0.003 1.3332)
	    (0.0031 0)
	    (0.01 0)
     );

    }

    outlet
    {
        type            fixedValue;
	value		uniform 0;

    }

    mantle
    {
        type            zeroGradient;
    }

}

